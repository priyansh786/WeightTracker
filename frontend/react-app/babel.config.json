{
    "presets": [
        "@babel/preset-env",
        ["@babel/preset-react",{
            "runtime":"automatic"
        }]
    ],
    "plugins": [
        "@babel/proposal-object-rest-spread",
        "@babel/transform-runtime",
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ]
    ]
}